{
  "name": "fairy-ui",
  "version": "0.1.3",
  "description": "fairy-ui 组件库",
  "main": "lib/fairyUi.umd.min.js",
  "keywords": [
    "fairy-ui",
    "vue",
    "ui"
  ],
  "author": "zhanhao",
  "files": [
    "lib/*",
    "packages"
  ],
  "scripts": {
    "lib": "vue-cli-service build --target lib --name fairyUi --dest lib packages/index.js",
